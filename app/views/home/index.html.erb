<%
schedules = {}
    User.all.each{ |user|
    if !user.schedule.nil? && !user.schedule.id.nil?
    schedules[user.schedule.id.to_s] = user.schedule
    end
    }

 %>
<%= schedules.keys %>

<%= db_config = YAML::load(File.read(File.join(Rails.root, "/config/mongo.yml"))) %>

<%= db_config[Rails.env]['adapter'] %>
